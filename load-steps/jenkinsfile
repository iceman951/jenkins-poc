def props = [:]
pipeline {
    agent any
    stages {

        stage("Initialisation") {
            steps { 
                script {
                    echo "fiest script"
                    load("load-steps/steps/step-initialisation.groovy").execute(props)
                } 
            }
        }

    }
  post {
    always { script { load("steps/step-post.groovy").execute(props) } }
  }
}
